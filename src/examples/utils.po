
half -> [a] [a]
half xs = !! xs * 0.5 (length)


head -> [a] [a]
head xs = !! xs 0 1

tail -> [a] [a]
tail xs = !! xs 0 (neg 1)

filter -> a -> [a] [a]
filter x xs =
  ? == xs [] []
  ? == [x] (head xs) (tail xs)
  ? T ++ (head xs) (filter x (tail xs))

filter' -> -> [a] AtomType -> [a] [a]
filter' f xs =
  ? == xs [] []
  ? == f (head xs) (tail xs)
  ? T ++ (head xs) (filter x (tail xs))
