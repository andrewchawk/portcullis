and -> Atom -> Atom Atom
and a b =
  ? == True a
    ? == True b
      True
    False
  False

fizzbuzz -> Num Num
fizzbuzz n =
  ? (and == 0 % n 3 == 0 % n 5) - 0 3
  ? == 0 % n 5
    - 0 2
  ? == 0 % n 3
    - 0 1
  n

fizzbuzzN -> Num [Num]
fizzbuzzN n =
  ? == n 0 Num []
  <+ (fizzbuzz n) (fizzbuzzN - n 1)
