#
id2 -> w -> [w] [w]
id2 x xs = xs

tail -> [f] [f]
tail xs =
  <+ xs xs id2
#


#
uncons -> [a] -> b -> -> a -> [a] b b
uncons as b f = b

tail2 -> -> w -> [w] [w] -> [f] [f]
tail2 f ns =
  (uncons ns ns f)
#

#
tail3 -> -> w -> [w] [w] -> [f] [f]
tail3 f ns =
  <+ ns ns f
